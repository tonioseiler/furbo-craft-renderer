{
    "name": "furbo/craft-renderer",
    "description": "Prerenders pages / entries",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "furbo renderer"
    ],
    "support": {},
    "license": "MIT",
    "authors": [
        {
            "name": "Tonio Seiler",
            "homepage": "https://furbo.ch/tonio-seiler-dipl-ing-fh-informatik"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "furbo/renderer": "1.0.0"
    },
    "autoload": {
        "psr-4": {
          "furbo\\furborenderer\\": "src/"
        }
    },
    "extra": {
        "name": "Furbo Renderer",
        "handle": "furbo-renderer",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "???",
        "class": "furbo\\furborenderer\\FurboRenderer"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../furbo-renderer/furbo-render-client"
        }
    ]
}
